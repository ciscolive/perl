#!/opt/perl/bin/perl

use strict;
use warnings;
#use lib::findbin '../lib'; # dev-only

# PODNAME: ebug_backend_perl
# ABSTRACT: Backend for ebug
our $VERSION = '0.63'; # VERSION


my $program = "@ARGV";
exec("$^X -Ilib -d:ebug::Backend $program");

__END__

