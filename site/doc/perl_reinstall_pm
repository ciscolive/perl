# 抓取脚本
perl -F'└──' -alne '$F[1] =~ s/\.\///; $F[1]=~ s/\//::/g; print $F[1]' perl_auto_install.so | perl -F/::/ -alne '$"="::", print "@F[0..$#F-1]" if /\w/' | uniq  | sort > perl_reinstall_pm

