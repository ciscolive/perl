\&     hardware_info   => [$hw1, $hw2, ...],                  # Optional / an array of Infoblox::Grid::Member::PreProvisioning::Hardware objects
\&     licenses         => [$lic1, $lic2, ...],                # Optional / Default value is undefined
\& );
